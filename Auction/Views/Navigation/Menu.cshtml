@model IEnumerable<Auction.Models.MenuModel>
<li>
    @Html.RouteLink("All",
        new
    {
        controller = "Lots",
        action = "List",
        page = 1
    })
</li>
@foreach (var link in Model)
{
    <li>
        @Html.RouteLink(link.CategoryName,
        new
    {
        controller = "Lots",
        action = "List",
        categoryId = link.CategoryId,
        page = 1
    })
    </li>
}
