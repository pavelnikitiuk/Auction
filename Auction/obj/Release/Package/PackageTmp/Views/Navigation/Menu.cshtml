@model IEnumerable<string>
<li>
    @Html.RouteLink("All",
        new
    {
        controller = "Lots",
        action = "List",
        page = 1
    })
</li>
@foreach (var link in Model)
{
    <li>
        @Html.RouteLink(link,
        new
    {
        controller = "Lots",
        action = "List",
        category = link,
        page = 1
    })
    </li>
}
